import math
if __name__ == '__main__' :
    t = int(input())
    while t >0 :
        t-=1
        s = input()
        s = list(s)
        i=0
        while i < len(s):
            cnt = 1
            while i+1 < len(s) and s[i] == s[i+1]:
                cnt +=1
                i +=1
            print(cnt,s[i],sep='',end='')
            i+=1
        print()



